$(document).ready(function() {
$(function () { 
    $('#container').highcharts({
        chart: {
            backgroundColor: '#FCFFC5',
            borderColor: '#EBBA95',
            borderWidth: 2,
            type: 'line'
        },
        title: {
            text: 'Salary Expenses by Months'
        },
        xAxis: {
            type: 'datetime',
            dateTimeLabelFormats: {
	week: '%b %y'
            },
            gridLineWidth: 2
        },
        yAxis: {
            title: {
                text: 'Thousands of Dollars'
            },
            gridLineWidth: 2
        },
        series: [{
            name: 'Salary',
	    data: gon.chart_data,
	    pointInterval: <%= 1.month * 1000 %>,
            pointStart: gon.point_start
        }]
    });
});
});
